{
  "manifest_version": 3,
  "name": "Redash default datasource",
  "version": "1.0.1",
  "description": "A default datasource selector for Redash",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "options_page": "options.html"
}
